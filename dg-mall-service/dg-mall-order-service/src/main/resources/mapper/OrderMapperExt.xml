<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//com.dg.mall.platform.mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dg.mall.service.order.mapper.OrderMapperExt">


    <insert id="insertOrder" parameterType="com.dg.mall.entity.OrderTbl" useGeneratedKeys="true" keyProperty="id">
        insert into order_tbl(user_id, commodity_code, `count`, money)
        values(#{userId}, #{commodityCode}, #{count}, #{money})
    </insert>


</mapper>
